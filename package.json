{
  "name": "brootaylor-v2",
  "version": "1.0.62",
  "description": "Version 2 of my personal website, brootaylor.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brootaylor/brootaylor-v2.git"
  },
  "bugs": {
    "url": "https://github.com/brootaylor/brootaylor-v2/issues"
  },
  "homepage": "https://github.com/brootaylor/brootaylor-v2#readme",
  "keywords": [
    "personal-website",
    "blog"
  ],
  "author": {
    "name": "Bruce Taylor",
    "url": "https://brootaylor.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "clean": "rimraf www && mkdir www",
    "prebuild": "npm run clean && echo 'Getting rid of old stuff first ðŸ”¥'",
    "js:docs": "./node_modules/.bin/jsdoc src/**/*.js -d docs/jsdocs",
    "js:docs-view": "npm run js:docs && cd docs/jsdocs && open index.html",
    "sass:docs": "sassdoc src/ -d docs/sassdocs",
    "sass:docs-view": "npm run sass:docs && cd docs/sassdocs && open index.html",
    "sass:tokens": "json-to-scss src/_data/tokens.json src/styles/abstracts/_tokens.scss",
    "sass:dev": "node-sass --output-style uncompressed --source-map true -o www/styles src/styles && npm run lint:sass",
    "sass:prod": "npm run sass:tokens && node-sass --output-style compressed --source-map true -o www/styles src/styles",
    "postcss": "postcss --map --no-map -r www/styles/*.css --config config/postcss.config.js",
    "lint:markdown": "markdownlint src/**/*.md",
    "lint:sass": "sass-lint 'src/**/*.scss' -v -q",
    "watch:files": "eleventy --serve",
    "watch:styles": "onchange 'src/**/*.scss' -- npm run build:styles-dev",
    "watch:scripts": "cross-env rollup -c config/rollup.config.js --watch",
    "watch": "npm-run-all --parallel watch:*",
    "build:files": "eleventy --quiet",
    "build:styles-dev": "npm run sass:dev && npm run postcss",
    "build:styles-prod": "npm run sass:prod && npm run postcss",
    "build:scripts": "rollup -c config/rollup.config.js",
    "build:docs": "npm run sass:docs && npm run js:docs",
    "build": "npm run build:styles-prod && npm run build:scripts && npm run build:files",
    "postbuild": "echo 'Everything has been built successfully... ðŸš€'",
    "deploy": "cross-env NODE_ENV=production npm run build",
    "audit:browser-support": "npx browserslist && echo 'ðŸ”† Modify .browserslistrc if and when browser support changes. ðŸ”†'",
    "audit:accessibility": "npx pa11y --standard WCAG2AA http://brootaylorv2.netlify.com",
    "audit:performance": "lighthouse https://brootaylor.com --budget-path=config/budget.json --output-path=./brootaylor-performance-results.html --view",
    "audit:npm-local": "ncu",
    "audit:npm-global": "ncu -g",
    "debug": "DEBUG=* npx eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.5",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "autoprefixer": "^9.8.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.2",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.5.0",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.4",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.12.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^6.1.0",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jsdoc": "^3.6.4",
    "markdownlint-cli": "^0.23.1",
    "minami": "^1.2.3",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "sass-lint": "^1.13.1",
    "sassdoc": "^2.7.1"
  }
}

{
  "name": "brootaylor-v2",
  "version": "1.0.66",
  "description": "Version 2 of my personal website, brootaylor.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brootaylor/brootaylor-v2.git"
  },
  "bugs": {
    "url": "https://github.com/brootaylor/brootaylor-v2/issues"
  },
  "homepage": "https://github.com/brootaylor/brootaylor-v2#readme",
  "keywords": [
    "personal-website",
    "blog"
  ],
  "author": {
    "name": "Bruce Taylor",
    "url": "https://brootaylor.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "clean": "rimraf www && mkdir www",
    "prebuild": "npm run clean && echo 'Getting rid of old stuff first ðŸ”¥'",
    "sass:tokens": "rimraf src/styles/tokens/*.scss && json-to-scss 'tokens/*.json' 'src/styles/tokens/_tokens.scss'",
    "sass:dev": "node-sass --output-style uncompressed --source-map true -o www/styles src/styles && npm run lint:sass",
    "sass:prod": "npm run sass:tokens && node-sass --output-style compressed --source-map true -o www/styles src/styles",
    "postcss": "postcss --map --no-map -r www/styles/*.css --config config/postcss.config.js",
    "lint:markdown": "markdownlint src/**/*.md",
    "lint:sass": "sass-lint 'src/**/*.scss' -v -q",
    "watch:files": "eleventy --serve",
    "watch:styles": "onchange 'src/**/*.scss' -- npm run build:styles-dev",
    "watch:scripts": "cross-env rollup -c config/rollup.config.js --watch",
    "watch": "npm-run-all --parallel watch:*",
    "build:files": "eleventy --quiet",
    "build:styles-dev": "npm run sass:dev && npm run postcss",
    "build:styles-prod": "npm run sass:prod && npm run postcss",
    "build:scripts": "rollup -c config/rollup.config.js",
    "build": "npm run build:styles-prod && npm run build:scripts && npm run build:files",
    "postbuild": "echo 'Everything has been built successfully... ðŸš€'",
    "deploy:build": "cross-env NODE_ENV=production npm run build",
    "deploy:draft": "netlify deploy",
    "deploy:production": "netlify deploy --prod",
    "audit:browser-support": "npx browserslist && echo 'ðŸ”† Modify .browserslistrc if and when browser support changes. ðŸ”†'",
    "audit:accessibility": "npx pa11y --standard WCAG2AA https://brootaylor.com",
    "audit:performance": "lighthouse https://brootaylor.com --budget-path=config/budget.json --output-path=./brootaylor-performance-results.html --view",
    "audit:vulnerabilities": "npx is-website-vulnerable https://brootaylor.com",
    "audit:npm-local": "ncu",
    "audit:npm-global": "ncu -g",
    "debug": "DEBUG=* npx eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@netlify/plugin-lighthouse": "^1.4.2",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "autoprefixer": "^10.2.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "faker": "^5.2.0",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.6.2",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.4",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.1",
    "rollup-plugin-terser": "^7.0.2",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "markdownlint-cli": "^0.26.0",
    "minami": "^1.2.3",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "sass-lint": "^1.13.1"
  }
}
